{
  "name": "chatbot",
  "packageManager": "pnpm@9.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start-app": "concurrently \"pnpm run start-docker\" \"pnpm run dev\" \"pnpm run start-server\"",
    "start-server": "python ../django_server/manage.py runserver",
    "start-docker": "docker-compose -f ../docker/docker-compose.yaml up -d",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint . --fix",
    "postinstall": "nuxt prepare",
    "preview": "nuxt preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@mdi/font": "^7.4.47",
    "@nuxt/types": "^2.18.1",
    "@nuxtjs/color-mode": "^3.4.2",
    "@pinia/nuxt": "^0.5.1",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@unocss/eslint-plugin": "^0.61.3",
    "@unocss/nuxt": "^0.61.0",
    "@vueuse/core": "^10.11.0",
    "@vueuse/head": "^2.0.0",
    "@vueuse/nuxt": "^10.11.0",
    "axios": "^1.7.7",
    "eslint": "^8.57.0",
    "jwt-decode": "^4.0.0",
    "nuxt": "^3.12.2",
    "pinia": "^2.1.7",
    "typescript": "^5.5.2",
    "unocss": "^0.61.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vue-tsc": "^1.8.27",
    "vuetify": "^3.6.10"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}